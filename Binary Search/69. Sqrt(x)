class Solution:
    def mySqrt(self, x: int) -> int:
        if x == 1:
            return 1
        left, right = 0, x // 2
        while left + 1 < right:
            mid = (left + right) // 2
            
            if mid * mid < x:
                left = mid
            else:
                right = mid
                
        if right * right <= x:
            return right
        else:
            return left
